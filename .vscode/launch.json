{
    // Use IntelliSense to learn about possible attributes.
    // Hover to view descriptions of existing attributes.
    // For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
    "version": "0.2.0",
    "configurations": [
        {
            "name": "syssim",
            "type": "cppdbg",
            "request": "launch",
            "program": "${workspaceFolder}/src/syssim",
            "args": [
                "${workspaceFolder}/valid/ascii.parv",
                "${workspaceFolder}/valid/test.outv",
                "${workspaceFolder}/valid/ascii.trace",
                "${workspaceFolder}/valid/power.cfg"
            ],
            "stopAtEntry": false,
            "cwd": "${workspaceFolder}",
            "environment": [],
            "externalConsole": false,
            "MIMode": "gdb",
            "setupCommands": [
                {
                    "description": "Enable pretty-printing for gdb",
                    "text": "-enable-pretty-printing",
                    "ignoreFailures": true
                }
            ]
        },
        {
            "name": "disksim",
            "type": "cppdbg",
            "request": "launch",
            "program": "${workspaceFolder}/src/disksim",
            "args": [
                "${workspaceFolder}/valid/ascii.parv",
                "${workspaceFolder}/valid/test.outv",
                "ascii",
                "${workspaceFolder}/valid/ascii.trace",
                "0",
                "${workspaceFolder}/valid/power.cfg"
            ],
            "stopAtEntry": false,
            "cwd": "${workspaceFolder}",
            "environment": [],
            "externalConsole": false,
            "MIMode": "gdb",
            "setupCommands": [
                {
                    "description": "Enable pretty-printing for gdb",
                    "text": "-enable-pretty-printing",
                    "ignoreFailures": true
                }
            ]
        }
    ]
}